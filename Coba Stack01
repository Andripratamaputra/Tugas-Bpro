import java.util.*;
public class CobaStack{
  
  public static void main(String[]args){
    Stack<String> coba = new Stack<String>();   
    coba.push("Pertama");
    coba.push("Kedua");
    coba.push("Ketiga");
    coba.push("Keempat");  
    int i=0;
    do{
      System.out.println("Stack yang keluar ke "+ i + " : " +coba.pop());
      i++;
    }while(coba.empty() == false);    
  }
}
